class Solution(object):
    def addDigits(self, n):
        if n <= 9:
            return n
        elif (n % 9) == 0:
            return 9
        else:
            return n % 9


def main():
    n = int(input('请输入一个正整数: '))
    solution = Solution()
    print(solution.addDigits(n))


if __name__ == '__main__':
    main()
